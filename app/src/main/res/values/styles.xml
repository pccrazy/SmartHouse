    </style>

    <style name="EditTextStyle" parent="Widget.AppCompat.EditText">
        <item name="colorControlNormal">@color/nliveo_orange_colorPrimary</item>
        <item name="colorControlActivated">@color/nliveo_orange_colorPrimaryDark</item>
        <item name="colorControlHighlight">@color/nliveo_orange_colorPrimaryDark</item>
    </style>

    <style name="myPDailog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/nliveo_orange_colorPrimary</item>
        <item name="android:textColorPrimary">#000000</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:windowTitleStyle">@style/TitleDialogTextStyle</item>
    </style>


    <style name="TitleDialogTextStyle" parent="Base.DialogWindowTitle.AppCompat">
        <item name="android:textColor">#3a85aa</item>
    </style>
    <!-- Base application theme. -->
    <style name="myDailog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <!-- Customize your theme here. -->
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/nliveo_transparent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="myDailogtransperent" parent="Theme.AppCompat.Light.Dialog.Alert">
        <!-- Customize your theme here. -->
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_height">300dp</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="MyMaterialTheme.Base" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="colorPrimary">@color/nliveo_orange_colorPrimary</item>
        <item name="colorPrimaryDark">@color/nliveo_orange_colorPrimaryDark</item>

        <item name="colorAccent">@color/nliveo_orange_colorPrimary</item>
    </style>

    <style name="Color1SwitchStyle">
        <item name="colorSwitchThumbNormal">@color/nliveo_orange_colorPrimary</item>
        <item name="colorControlActivated">@color/nliveo_orange_colorPrimaryDark</item>
        <item name="colorControlNormal">@color/nliveo_orange_colorPrimary</item>
        <item name="colorControlHighlight">@color/nliveo_orange_colorPrimary</item>
    </style>

    <style name="myTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="android:editTextStyle">@style/EditTextStyle</item>
        <item name="colorPrimary">@color/nliveo_orange_colorPrimary</item>
        <item name="colorPrimaryDark">@color/nliveo_orange_colorPrimaryDark</item>
        <item name="colorAccent">@color/nliveo_orange_colorPrimary</item>
        <item name="colorControlNormal">@color/nliveo_orange_colorPrimary</item>
        <item name="colorControlActivated">@color/nliveo_orange_colorPrimary</item>
        <item name="colorControlHighlight">@color/nliveo_orange_colorPrimary</item>
    </style>

    <style name="myTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:editTextStyle">@style/EditTextStyle</item>
        <item name="colorPrimary">@color/nliveo_orange_colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/nliveo_orange_colorPrimaryDark</item>
        <item name="colorAccent">@color/nliveo_orange_colorPrimary</item>
        <item name="colorSwitchThumbNormal">@color/nliveo_orange_colorPrimary</item>

        <item name="colorControlNormal">@color/nliveo_orange_colorPrimary</item>
        <item name="colorControlActivated">@color/nliveo_orange_colorPrimary</item>
        <item name="colorControlHighlight">@color/nliveo_orange_colorPrimary</item>
    </style>


